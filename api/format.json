---
title: Format API
parent: API
---
{
{% for page in site.format %}"{{ page.slug }}": {
  "slug": "{{ page.slug }}",
  "title": "{{ page.title }}",
  "issue": "{{ page.issue }}",
  "description": "{{ page.description }}",
  "url": "{{ site.baseurl }}{{ page.url }}",
  "owner": "{{ page.owner }}",
  "base_type": "{{ page.base_type" }}"
}{% unless forloop.last %},{% endunless %}
{% endfor %}
}
