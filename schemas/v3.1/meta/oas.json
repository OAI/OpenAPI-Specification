      "description": {
        "type": "string"
      },
      "type": {
        "type": "string",
        "enum": [
          "http"
        ]
      }
    },
    "patternProperties": {
      "^x-": {
      }
    },
    "additionalProperties": false,
    "oneOf": [
      {
        "description": "Bearer",
        "properties": {
