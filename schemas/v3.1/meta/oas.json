      },
      "url": {
        "type": "string",
        "format": "uri-reference"
      }
    },
    "patternProperties": {
      "^x-": {
      }
    },
    "additionalProperties": false
  },
  "ExampleXORExamples": {
    "description": "Example and examples are mutually exclusive",
    "not": {
      "required": [
        "example",
        "examples"
      ]
    }
  },
  "SchemaXORContent": {
    "description": "Schema and content are mutually exclusive, at least one is required",
    "not": {
      "required": [
        "schema",
        "content"
      ]
    },
    "oneOf": [
      {
        "required": [
          "schema"
        ]
      },
      {
        "required": [
          "content"
        ],
        "description": "Some properties are not allowed if content is present",
        "allOf": [
          {
            "not": {
              "required": [
                "style"
              ]
            }
          },
          {
            "not": {
              "required": [
                "explode"
              ]
            }
          },
          {
            "not": {
              "required": [
                "allowReserved"
              ]
            }
          },
          {
            "not": {
              "required": [
                "example"
              ]
            }
          },
          {
            "not": {
              "required": [
                "examples"
              ]
            }
          }
        ]
      }
    ]
  },
