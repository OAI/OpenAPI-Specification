        }
      },
      {
        "description": "Parameter in header",
        "properties": {
          "in": {
            "enum": [
              "header"
            ]
          },
          "style": {
            "enum": [
              "simple"
            ],
            "default": "simple"
          }
        }
      },
      {
        "description": "Parameter in cookie",
        "properties": {
          "in": {
            "enum": [
              "cookie"
            ]
          },
          "style": {
            "enum": [
              "form"
            ],
            "default": "form"
          }
        }
      }
    ]
  },
