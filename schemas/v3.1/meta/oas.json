      "allowEmptyValue": {
        "type": "boolean",
        "default": false
      },
      "style": {
        "type": "string",
        "enum": [
          "simple"
        ],
        "default": "simple"
      },
      "explode": {
        "type": "boolean"
      },
      "allowReserved": {
        "type": "boolean",
        "default": false
