      "parameters": {
        "type": "array",
        "items": {
          "oneOf": [
            {
              "$ref": "#/$defs/Parameter"
            },
            {
              "$ref": "#/$defs/Reference"
            }
          ]
        },
        "uniqueItems": true
      },
      "requestBody": {
        "oneOf": [
          {
            "$ref": "#/$defs/RequestBody"
          },
          {
            "$ref": "#/$defs/Reference"
          }
        ]
      },
      "responses": {
        "$ref": "#/$defs/Responses"
      },
      "callbacks": {
        "type": "object",
        "additionalProperties": {
          "oneOf": [
            {
              "$ref": "#/$defs/Callback"
            },
            {
              "$ref": "#/$defs/Reference"
            }
          ]
        }
      },
      "deprecated": {
        "type": "boolean",
        "default": false
      },
      "security": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SecurityRequirement"
        }
      },
      "servers": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Server"
        }
      }
    },
    "patternProperties": {
      "^x-": {
      }
    },
    "additionalProperties": false
