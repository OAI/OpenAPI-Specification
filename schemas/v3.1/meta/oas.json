  "Responses": {
    "type": "object",
    "properties": {
      "default": {
        "oneOf": [
          {
            "$ref": "#/$defs/Response"
          },
          {
            "$ref": "#/$defs/Reference"
          }
        ]
      }
    },
    "patternProperties": {
      "^[1-5](?:\\d{2}|XX)$": {
        "oneOf": [
          {
            "$ref": "#/$defs/Response"
          },
          {
            "$ref": "#/$defs/Reference"
          }
        ]
      },
      "^x-": {
      }
    },
    "minProperties": 1,
    "additionalProperties": false
