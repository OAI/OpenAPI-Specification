          "oneOf": [
            {
              "$ref": "#/$defs/Parameter"
            },
            {
              "$ref": "#/$defs/Reference"
            }
          ]
        },
        "uniqueItems": true
      }
    },
    "patternProperties": {
      "^(get|put|post|delete|options|head|patch|trace)$": {
        "$ref": "#/$defs/Operation"
      },
      "^x-": {
      }
    },
    "additionalProperties": false
  },
